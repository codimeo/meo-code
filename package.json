{
  "name": "meo-studio",
  "description": "Meo-Studio aims to become the world's most proficient Visual Coding environment",
  "version": "1.0.0",
  "homepage": "https://meo.codimeo.com",
  "author": {
    "name": "TopNiz",
    "email": "nizar.ayed@upgrade-code.org",
    "url": "http://upgrade-code.org"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/codimeo/meo-studio.git"
  },
  "bugs": {
    "url": "https://github.com/codimeo/meo-studio/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://meo.codimeo.com/blob/master/LICENSE-MIT"
    }
  ],
  "main": "meo",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "build": "grunt",
    "clean": "grunt clean",
    "update_submodules": "grunt update_submodules",
    "test": "grunt nodeunit"
  },
  "devDependencies": {
    "grunt": ">=0.4.5",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "~0.6.0",
    "grunt-contrib-nodeunit": "~0.2.0",
    "grunt-contrib-uglify": "~0.2.0",
    "grunt-contrib-watch": "~0.4.0",
    "grunt-eslint": "^21.0.0",
    "grunt-update-submodules": "^0.4.1"
  },
  "keywords": [],
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        "error",
        4
      ],
      "max-len": [
        "error",
        78
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": "off",
      "semi": [
        "error",
        "always"
      ],
      "strict": "off",
      "guard-for-in": "off",
      "no-eval": "off"
    }
  }
}
